{
  "Ks": 3,
  "Kt": 3,
  "blocks": [[1, 32, 64], [64, 32, 128]],
  "dropout": 0,
  "graph_conv_type": "chebconv",

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "add_time_in_day": false,
  "add_day_in_week": true,

  "max_epoch": 50,
  "learner": "adam",
  "learning_rate": 0.001,
  "weight_decay":0.001,
    "lr_decay":true,
    "lr_scheduler":"StepLR",
  "step_size": 20,
    "lr_decay_ratio":0.3,
  "clip_grad_norm": false,
  "use_early_stop": true
}
